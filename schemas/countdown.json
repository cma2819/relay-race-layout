{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "raw": {
      "type": "integer",
      "default": 0
    },
    "formatted": {
      "type": "string",
      "default": "00:00"
    },
    "timestamp": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null
    }
  },
  "required": ["raw", "formatted", "timestamp"]
}
